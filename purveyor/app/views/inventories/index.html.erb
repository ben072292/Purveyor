<h1>Listing inventories</h1>

<table> cellspacing="10"> 
  <tr>
    <th>Name</th>
    <th>Cost</th>
    <th>Quantity</th>
    <th>Unit</th>
    <th>Expiration Date</th>
    <th>Growing Practice</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>
  
  
  <% @inventory_list.each do |inventory| %>

  <tr>
    <td><%= inventory.name%></td>
    <td><%= inventory.cost %></td>
    <td><%= inventory.quantity %></td>
    <td><%= inventory.unit %></td>
    <td><%= inventory.expirationDate %></td>
    <td><%= inventory.growingPractice %></td>
    <td><%= link_to 'Show', inventory %></td>
    <td><%= link_to 'Edit', edit_inventory_path(inventory) %></td>
    <td><%= link_to 'Destroy', inventory, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
  <% end %>
</table>
